        "gradeRightDecs": {
          "type": "string"
        },
        "gradeRightIcon": {
          "type": "string"
        },
        "gradeRightTitle": {
          "type": "string"
        },
        "gradeRightExplain": {
          "type": "string"
        }
      },
      "required": [
        "gradeRightName",
        "gradeRightDecs",
        "gradeRightIcon",
        "gradeRightTitle",
        "gradeRightExplain"
      ],
      "additionalProperties": false
    }
