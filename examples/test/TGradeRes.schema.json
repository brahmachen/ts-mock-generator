              "gradeIcon": {
                "type": "string"
              },
              "gradeCode": {
                "type": "number"
              },
              "gradeName": {
                "type": "string"
              },
              "gradeRightDecs": {
                "type": "string"
              },
              "upgradePrompt": {
                "type": "string"
              },
              "gradeRightAcount": {
                "type": "number"
              },
              "gradeRightList": {
                "type": "array",
                "items": {
